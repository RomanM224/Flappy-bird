<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>StartGame</servlet-name>
  	<display-name>StartGame</display-name>
  	<description></description>
  	<servlet-class>com.maistruk.flappybird.StartGame</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FinishGame</servlet-name>
  	<display-name>FinishGame</display-name>
  	<description></description>
  	<servlet-class>com.maistruk.flappybird.FinishGame</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>StartGame</servlet-name>
  	<url-pattern>/StartGame</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FinishGame</servlet-name>
  	<url-pattern>/FinishGame</url-pattern>
  </servlet-mapping>
</web-app>
